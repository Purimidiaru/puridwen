#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/paul/dev/puridwen/node_modules/.pnpm/@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules/@sveltejs/kit/node_modules:/home/paul/dev/puridwen/node_modules/.pnpm/@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules/@sveltejs/node_modules:/home/paul/dev/puridwen/node_modules/.pnpm/@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules:/home/paul/dev/puridwen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/paul/dev/puridwen/node_modules/.pnpm/@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules/@sveltejs/kit/node_modules:/home/paul/dev/puridwen/node_modules/.pnpm/@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules/@sveltejs/node_modules:/home/paul/dev/puridwen/node_modules/.pnpm/@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules:/home/paul/dev/puridwen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../../../../@sveltejs+kit@2.22.3_@sveltejs+vite-plugin-svelte@5.1.0_svelte@5.35.5_vite@6.3.5__svelte@5.35.5_vite@6.3.5/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
fi
